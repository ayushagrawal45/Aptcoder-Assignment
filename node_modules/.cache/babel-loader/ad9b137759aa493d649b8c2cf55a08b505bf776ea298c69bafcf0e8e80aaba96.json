{"ast":null,"code":"import axios from 'axios'; // Import axios for HTTP requests\nimport TaskList from './components/TaskList.vue';\nexport default {\n  name: 'App',\n  components: {\n    TaskList\n  },\n  data() {\n    return {\n      tasks: [],\n      newTask: ''\n    };\n  },\n  methods: {\n    async addTask() {\n      try {\n        const newTask = {\n          title: this.newTask,\n          createdAt: new Date(),\n          completed: false\n        };\n        await axios.post('http://localhost:3000/tasks', newTask); // Adjust URL as needed\n        this.fetchTasks();\n        this.newTask = '';\n      } catch (error) {\n        console.error('Error adding task:', error);\n      }\n    },\n    async completeTask(id) {\n      try {\n        await axios.patch(`http://localhost:3000/tasks/${id}`, {\n          completed: true,\n          completedAt: new Date()\n        });\n        this.fetchTasks();\n      } catch (error) {\n        console.error('Error completing task:', error);\n      }\n    },\n    async fetchTasks() {\n      try {\n        const response = await axios.get('http://localhost:3000/tasks'); // Adjust URL as needed\n        this.tasks = response.data;\n      } catch (error) {\n        console.error('Error fetching tasks:', error);\n      }\n    }\n  },\n  mounted() {\n    this.fetchTasks();\n  }\n};","map":{"version":3,"names":["axios","TaskList","name","components","data","tasks","newTask","methods","addTask","title","createdAt","Date","completed","post","fetchTasks","error","console","completeTask","id","patch","completedAt","response","get","mounted"],"sources":["C:\\Users\\91798\\OneDrive\\Desktop\\pj c++\\vuejs\\AptCoder-Assignment\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <div class=\"header\">\r\n      <h1>To-Do App</h1>\r\n      <img src=\"todo-icon.png\" alt=\"Todo Icon\" class=\"todo-icon\">\r\n    </div>\r\n    <div class=\"container\">\r\n      <div class=\"add-task\">\r\n        <h2>Add Task</h2>\r\n        <div class=\"input-wrapper\">\r\n          <input v-model=\"newTask\" placeholder=\"Add New task...\" />\r\n          <button @click=\"addTask\">Add</button>\r\n        </div>\r\n      </div>\r\n      <div class=\"task-list-wrapper\">\r\n        <transition-group name=\"fade\">\r\n          <task-list :tasks=\"tasks\" @completeTask=\"completeTask\"></task-list>\r\n        </transition-group>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'; // Import axios for HTTP requests\r\nimport TaskList from './components/TaskList.vue';\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    TaskList,\r\n  },\r\n  data() {\r\n    return {\r\n      tasks: [],\r\n      newTask: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async addTask() {\r\n      try {\r\n        const newTask = { title: this.newTask, createdAt: new Date(), completed: false };\r\n        await axios.post('http://localhost:3000/tasks', newTask); // Adjust URL as needed\r\n        this.fetchTasks();\r\n        this.newTask = '';\r\n      } catch (error) {\r\n        console.error('Error adding task:', error);\r\n      }\r\n    },\r\n    async completeTask(id) {\r\n      try {\r\n        await axios.patch(`http://localhost:3000/tasks/${id}`, { completed: true, completedAt: new Date() });\r\n        this.fetchTasks();\r\n      } catch (error) {\r\n        console.error('Error completing task:', error);\r\n      }\r\n    },\r\n    async fetchTasks() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/tasks'); // Adjust URL as needed\r\n        this.tasks = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching tasks:', error);\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchTasks();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Your CSS styles */\r\n.fade-enter-active, .fade-leave-active {\r\n  transition: opacity 0.5s;\r\n}\r\n\r\n.fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\r\n  opacity: 0;\r\n}\r\n</style>\r\n"],"mappings":"AAwBA,OAAOA,KAAI,MAAO,OAAO,EAAE;AAC3B,OAAOC,QAAO,MAAO,2BAA2B;AAEhD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,OAAOA,CAAA,EAAG;MACd,IAAI;QACF,MAAMF,OAAM,GAAI;UAAEG,KAAK,EAAE,IAAI,CAACH,OAAO;UAAEI,SAAS,EAAE,IAAIC,IAAI,CAAC,CAAC;UAAEC,SAAS,EAAE;QAAM,CAAC;QAChF,MAAMZ,KAAK,CAACa,IAAI,CAAC,6BAA6B,EAAEP,OAAO,CAAC,EAAE;QAC1D,IAAI,CAACQ,UAAU,CAAC,CAAC;QACjB,IAAI,CAACR,OAAM,GAAI,EAAE;MACnB,EAAE,OAAOS,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC;MAC5C;IACF,CAAC;IACD,MAAME,YAAYA,CAACC,EAAE,EAAE;MACrB,IAAI;QACF,MAAMlB,KAAK,CAACmB,KAAK,CAAE,+BAA8BD,EAAG,EAAC,EAAE;UAAEN,SAAS,EAAE,IAAI;UAAEQ,WAAW,EAAE,IAAIT,IAAI,CAAC;QAAE,CAAC,CAAC;QACpG,IAAI,CAACG,UAAU,CAAC,CAAC;MACnB,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD;IACF,CAAC;IACD,MAAMD,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMO,QAAO,GAAI,MAAMrB,KAAK,CAACsB,GAAG,CAAC,6BAA6B,CAAC,EAAE;QACjE,IAAI,CAACjB,KAAI,GAAIgB,QAAQ,CAACjB,IAAI;MAC5B,EAAE,OAAOW,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF;EACF,CAAC;EACDQ,OAAOA,CAAA,EAAG;IACR,IAAI,CAACT,UAAU,CAAC,CAAC;EACnB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}